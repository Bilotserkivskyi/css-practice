<footer>
    {{#with footer}}
        <div class="footer">
            <div>
                {{#each pages}}
                    <div class="footer_name">
                        <span>{{name}}</span>
                    </div>
                {{/each}}
            </div>
            
            <div>
                <span class="footer_copyright">{{copyright}}</span>
            </div>
        </div>
    {{/with}}
</footer> 